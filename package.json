{
  "name": "mu-shaper",
  "version": "0.0.1",
  "description": "Node.js开发脚手架，快速开发，一站式开发，轻量级，更简洁的Node模块化模板",
  "author": "XiaoMuCOOL",
  "private": false,
  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon -e ts --exec ts-node ./server/app.ts",
    "bulid": "ts-node ./test/decorator.spec.ts",
    "lint": "standardx --fix **/*.ts",
    "docs": "vuepress dev docs",
    "test": "jest --coverage --preset ts-jest ./test/"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "koa": "^2.11.0",
    "koa-joi-router": "^6.0.1",
    "koa-onerror": "^4.1.0",
    "mysql2": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.21",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.24",
    "@types/koa": "^2.11.0",
    "@types/koa-joi-router": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "@vuepress/plugin-back-to-top": "^1.2.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "standardx": "^5.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "vuepress": "^1.2.0"
  },
  "eslintConfig": {
    "rules": {
      "no-inner-declarations": "off"
    }
  },
  "standardx": {
    "globals": [
      "test",
      "expect"
    ],
    "ignore": [
      "node_modules/**.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingblue/mu-shaper.git"
  },
  "homepage": "https://github.com/bingblue/mu-shaper#readme",
  "readme": "ERROR: No README data found!"
}
